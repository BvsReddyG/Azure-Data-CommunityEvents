								},
								"sink": {
									"name": "Code",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "Airline_Code",
									"type": "String"
								},
								"sink": {
									"name": "Airline_Code",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_60e",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_60e",
						"type": "DatasetReference"
					}
